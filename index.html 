import subprocess

app =3D Flask(__name__)

@app.route('/chat', methods=3D['POST'])
def chat():
    user_input =3D request.json['message']

    try:
        result =3D subprocess.run(
            ['ollama', 'run', 'phi', user_input],
            stdout=3Dsubprocess.PIPE,
            stderr=3Dsubprocess.PIPE,
            text=3DTrue
        )
        response =3D result.stdout.strip()
        return jsonify({'response': response})

    except Exception as e:
        return jsonify({'error': str(e)}), 500

if __name__ =3D=3D '__main__':
    app.run(port=3D5000)




